  else
    goto <bb 49>; [INV]

  <bb 49> :
  # DEBUG BEGIN_STMT
  if (addr != 0B)
    goto <bb 50>; [INV]
  else
    goto <bb 57>; [INV]

  <bb 50> :
  # DEBUG BEGIN_STMT
  _57 = current_block_index & 1;
  if (_57 != 0)
    goto <bb 51>; [INV]
  else
    goto <bb 52>; [INV]

  <bb 51> :
  # DEBUG BEGIN_STMT
  addr_index.4_58 = addr_index;
  addr_index = addr_index.4_58 + 1;
  addr_index.5 = addr_index.4_58;
  _59 = addr->addr[addr_index.5];
  _60 = current_block_value | _59;
  addr->addr[addr_index.5] = _60;
  goto <bb 53>; [INV]

  <bb 52> :
  # DEBUG BEGIN_STMT
  _61 = current_block_value << 16;
  addr->addr[addr_index] = _61;

  <bb 53> :
fix_byte_order_and_return:
  # DEBUG BEGIN_STMT
  addr_index = 0;
  goto <bb 55>; [INV]

  <bb 54> :
  # DEBUG BEGIN_STMT
  _62 = addr->addr[addr_index];
  _63 = lwip_htonl (_62);
  addr->addr[addr_index] = _63;
  # DEBUG BEGIN_STMT
  addr_index = addr_index + 1;

  <bb 55> :
  # DEBUG BEGIN_STMT
  if (addr_index <= 3)
    goto <bb 54>; [INV]
  else
    goto <bb 56>; [INV]

  <bb 56> :
  # DEBUG BEGIN_STMT
  addr->zone = 0;

  <bb 57> :
  # DEBUG BEGIN_STMT
  if (current_block_index != 7)
    goto <bb 58>; [INV]
  else
    goto <bb 59>; [INV]

  <bb 58> :
  # DEBUG BEGIN_STMT
  D.6443 = 0;
  // predicted unlikely by early return (on trees) predictor.
  goto <bb 60>; [INV]

  <bb 59> :
  # DEBUG BEGIN_STMT
  D.6443 = 1;

  <bb 60> :
  return D.6443;

}


